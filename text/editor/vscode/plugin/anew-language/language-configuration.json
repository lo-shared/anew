{
    "autoClosingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "`",
            "`"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "comments": {
        "blockComment": [
            "<@>",
            "<.>"
        ],
        "lineComment": "(@)"
    },
    "folding": {
        "markers": {
            "end": "^[-][-]",
            "start": "^[*:][*:]"
        }
    },
    "indentationRules": {
        "decreaseIndentPattern": "(^[\\s]+[\\-:*][ ])",
        "increaseIndentPattern": "(^[\\-:*][ ])"
    },
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ],
        [
            "`",
            "`"
        ]
    ],
    "wordPattern": "[#+-]?[`][\\s\\S]+?[`]|[^~`!@#$%^&*()_\\-+=\\{\\[\\}\\]|\\:;\"'<,>.?/\\s]+"
}